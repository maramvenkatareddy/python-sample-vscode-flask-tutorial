pipeline {
 
agent any
 
stages{
  stage('cloneeeeeeee'){
 
    steps{
 
      git branch:'master', url:'https://github.com/maramvenkatareddy/python-sample-vscode-flask-tutorial.git'
    }
  } 
  stage('codebuild'){
    steps{
       sh '''
            cd python-sample-vscode-flask-tutorial/
            pip install -r requirements.txt
        '''
    }
  }
}
}
